{
  "name": "word-persona-feedback",
  "version": "1.3.0",
  "private": true,
  "description": "Word Add-in for persona-based LLM feedback",
  "config": {
    "baseUrl": "https://YOUR-VERCEL-APP.vercel.app"
  },

  "scripts": {
    "start": "webpack serve --mode development --port 3000",
    "build": "webpack --mode production",
    "manifest": "node scripts/make-manifest.cjs",
    "postbuild": "npm run manifest && node -e \"require('fs').copyFileSync('manifest.dev.xml','dist/manifest.xml')\"",
    "build:vercel": "npm run build && npm run postbuild"
  }

,
  "dependencies": {},
  "devDependencies": {
    "@types/office-js": "^1.0.389",
    "copy-webpack-plugin": "^12.0.0",
    "css-loader": "^7.1.2",
    "html-loader": "^5.1.0",
    "html-webpack-plugin": "^5.6.0",
    "style-loader": "^4.0.0",
    "ts-loader": "^9.5.1",
    "typescript": "^5.6.3",
    "webpack": "^5.95.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.2"
  }
}